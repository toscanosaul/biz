\begin_layout Paragraph*
Proposition 4.
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $W=\left(W_{1},\ldots,W_{k}\right)$
\end_inset

 be a 
\begin_inset Formula $k-$
\end_inset

dimensional independent standard Brownian motion.
 Let 
\begin_inset Formula $q_{tx}^{W}\left(A\right):=\mbox{exp}\left(\frac{1}{\lambda_{z}^{2}}\left(\lambda_{z}W_{x}\left(t\right)+ta_{x}\right)\right)/\sum_{x^{'}\in A}\mbox{exp}\left(\frac{1}{\lambda_{z}^{2}}\left(\lambda_{z}W_{x^{'}}\left(t\right)+ta_{x^{'}}\right)\right)$
\end_inset

 for all 
\begin_inset Formula $x\in A$
\end_inset

 and 
\begin_inset Formula $A\subset\left\{ 1,\ldots,k\right\} $
\end_inset

.
 Fix 
\begin_inset Formula $m\in\left\{ 1,\ldots,k-1\right\} $
\end_inset

.
 We have that for all 
\begin_inset Formula $N\in\mathbb{N}$
\end_inset

, there exists 
\begin_inset Formula $t$
\end_inset

 such that 
\begin_inset Formula $\tau_{m}+\frac{1}{N}\geq t>\tau_{m}$
\end_inset

 and 
\begin_inset Formula $q_{tx}^{W}\left(A_{m-1}\right)>P_{m-1}$
\end_inset

 for some 
\begin_inset Formula $x\in A_{m-1}$
\end_inset

 almost surely given that 
\begin_inset Formula $M=m$
\end_inset

.
 (See the definitions in p.
 4).
\end_layout

\begin_layout Paragraph*
Proof.
\end_layout

\begin_layout Standard
The proof has the same spirit than the proof of Lemma 4 of Frazier 
\begin_inset CommandInset citation
LatexCommand cite
key "key-5"

\end_inset

, there Frazier proved that 
\begin_inset Formula $\tau_{m}<\infty$
\end_inset

 almost surely.
 Furthermore, 
\begin_inset Formula $M\leq k-1$
\end_inset

 almost surely.
 We should note that the Lemma 4 supposes that all systems have the same
 variance which is the case here because 
\begin_inset Formula $\mbox{Var}\left(\lambda_{z}W_{x}\left(t\right)+ta_{x}\right)=\lambda_{z}^{2}t$
\end_inset

.
 We condition on the event 
\begin_inset Formula $\mathcal{S}$
\end_inset

 such that the previous properties hold and has probability 
\begin_inset Formula $1$
\end_inset

.
 
\end_layout

\begin_layout Standard
Define 
\begin_inset Formula $a=\lambda_{z}\mbox{log}\left(\frac{P_{m-1}\left(k-1\right)}{1-P_{m-1}}\right)$
\end_inset

.
 If 
\begin_inset Formula $m=1$
\end_inset

, then 
\begin_inset Formula $P_{m-1}=P^{*}\in\left(0,1\right)$
\end_inset

, and so 
\begin_inset Formula $a$
\end_inset

 is finite.
 If 
\begin_inset Formula $m>1$
\end_inset

, then 
\begin_inset Formula 
\[
P^{*}\leq P_{m-1}\leq P^{*}/\left(1-c\right)^{m-1}\leq P^{*}/\left(1-c\right)^{k-2}\leq P^{*}/\left(P^{*}\right)^{k-2/k-1}<1
\]

\end_inset

and so 
\begin_inset Formula $P_{m-1}\in\left(0,1\right)$
\end_inset

 and 
\begin_inset Formula $a$
\end_inset

 is finite.
\end_layout

\begin_layout Standard
We define 
\begin_inset Formula $T^{*}=\mbox{inf}\left\{ t\geq\tau_{m}:q_{t,x}^{W}\left(A_{m-1}\right)>P_{m-1}\mbox{ for some }x\in A_{m-1}\right\} $
\end_inset

.
 Fix 
\begin_inset Formula $n\in\mathbb{N}$
\end_inset

, 
\begin_inset Formula $n>2$
\end_inset

, and define 
\begin_inset Formula $\tau=\tau_{m}+\frac{t^{*}}{nN}$
\end_inset

 for a deterministic 
\begin_inset Formula $t^{*}\in\left\{ 1,\ldots,n-1\right\} $
\end_inset

.
 Let 
\begin_inset Formula $x$
\end_inset

 be any 
\begin_inset Formula $\mathcal{F}_{\tau_{m}}$
\end_inset

-measurable random variable that is almost surely in 
\begin_inset Formula $A_{m-1}$
\end_inset

 and we define 
\begin_inset Formula $\Gamma_{t,x}=W_{x}\left(t\right)+t\frac{a_{x}}{\lambda_{z}}$
\end_inset

.
 Consider the event 
\begin_inset Formula $ $
\end_inset


\begin_inset Formula $a<\Gamma_{\tau+\frac{1}{nN},x}-\Gamma_{\tau+\frac{1}{nN},y}$
\end_inset

 for each 
\begin_inset Formula $y\in A_{m-1}-\left\{ x\right\} $
\end_inset

.
 On this event, 
\begin_inset Formula $q_{\tau+\frac{1}{mN},y}^{W}\left(A_{m-1}\right)/q_{\tau+\frac{1}{mN},x}^{W}\left(A_{m-1}\right)=\mbox{exp}\left(\frac{1}{\lambda_{z}}\left(\Gamma_{\tau+\frac{1}{nN},y}-\Gamma_{\tau+\frac{1}{nN},x}\right)\right)<\mbox{exp}\left(-a/\lambda_{z}\right)$
\end_inset

 for 
\begin_inset Formula $ $
\end_inset

 
\begin_inset Formula $y\in A_{m-1}-\left\{ x\right\} $
\end_inset

 and
\begin_inset Formula 
\[
q_{\tau+\frac{1}{nN},x}\left(A_{m-1}\right)=\left[1+\sum_{y\in A_{m-1}-\left\{ x\right\} }q_{\tau+\frac{1}{nN},y}\left(A_{m-1}\right)/q_{\tau+\frac{1}{nN},x}\left(A_{m-1}\right)\right]^{-1}>\left[1+\left(k-1\right)\mbox{exp}\left(-a/\lambda_{z}\right)\right]^{-1}=P_{m-1}.
\]

\end_inset

Thus, on the event considered, 
\begin_inset Formula $T^{*}\leq\tau+\frac{1}{nN}$
\end_inset

.
 
\end_layout

\begin_layout Standard
We now define 
\begin_inset Formula $\tilde{x}\in\mbox{arg max}_{x\in A_{m-1}}\mbox{ }\Gamma_{\tau,x}$
\end_inset

, which is 
\begin_inset Formula $\mathcal{F}_{\tau}$
\end_inset

-measurable and is almost surely in 
\begin_inset Formula $A_{m-1}$
\end_inset

.
 Then we have that
\begin_inset Formula 
\begin{eqnarray*}
\mathbb{P}\left\{ T^{*}\leq\tau+\frac{1}{nN}\mid\mathcal{F}_{\tau},T^{*}>\tau\right\}  & \geq & \mathbb{P}\left\{ a<\Gamma_{\tau+\frac{1}{nN},\tilde{x}}-\Gamma_{\tau+\frac{1}{nN},x}\mbox{ \mbox{}}\forall x\in A_{m-1}-\left\{ \tilde{x}\right\} \mid\mathcal{F}_{\tau},T^{*}>\tau\right\} \\
 & \geq & \mathbb{P}\left\{ \Gamma_{\tau+\frac{1}{nN},\tilde{x}}\geq\Gamma_{\tau,\tilde{x}},\Gamma_{\tau,\tilde{x}}-\Gamma_{\tau+\frac{1}{nN},x}>a\mbox{ \mbox{}}\forall x\in A_{m-1}-\left\{ \tilde{x}\right\} \mid\mathcal{F}_{\tau},T^{*}>\tau\right\} \\
 & \geq & \mathbb{P}\left\{ \Gamma_{\tau+\frac{1}{nN},\tilde{x}}\geq\Gamma_{\tau,\tilde{x}},\Gamma_{\tau,x}-\Gamma_{\tau+\frac{1}{nN},x}>a\mbox{ \mbox{}}\forall x\in A_{m-1}-\left\{ \tilde{x}\right\} \mid\mathcal{F}_{\tau},T^{*}>\tau\right\} \\
 & = & \mathbb{P}\left\{ \Gamma_{\tau+\frac{1}{nN},\tilde{x}}\geq\Gamma_{\tau_{m},\tilde{x}}\mid\mathcal{F}_{\tau}\right\} \prod_{x\in A_{m-1}\setminus\left\{ \tilde{x}\right\} }\mathbb{P}\left\{ \Gamma_{\tau,x}-\Gamma_{\tau+\frac{1}{nN},x}>a\mid\mathcal{F}_{\tau}\right\} .
\end{eqnarray*}

\end_inset

Note that 
