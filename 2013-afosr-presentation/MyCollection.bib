@article{RoHa02,
abstract = {Phase I clinical trials are typically small, uncontrolled studies
designed to determine a maximum tolerated dose of a drug which will
be used in further testing. Two divergent schools have developed
in designing phase I clinical trials. The first defines the maximum
tolerated dose as a statistic computed from data, and hence it is
identified, rather than estimated. The second defines the maximum
tolerated dose as a parameter of a monotonic dose-response curve,
and hence is estimated. We review techniques from both philosophies.
The goal is to present these methods in a single package, to compare
them from philosophical and statistical grounds, to hopefully clear
up some common misconceptions, and to make a few recommendations.
This paper is not a review of simulation studies of these designs,
nor does it present any new simulations comparing these designs.},
annote = {2007.11.26},
author = {Rosenberger, William F and Haines, Linda M},
doi = {10.1002/sim.1229},
file = {:Users/pfrazier/work/papers/RoHa02.pdf:pdf},
journal = {Stat Med},
keywords = { Drug; Humans; Maximum Tolerated Dose; Research De, Phase I; Dose-Response Relationship,Clinical Trials},
month = sep,
number = {18},
pages = {2757--2770},
pmid = {12228889},
title = {{Competing designs for phase I clinical trials: a review.}},
url = {http://dx.doi.org/10.1002/sim.1229},
volume = {21},
year = {2002}
}
