keywords = {Adsorption,Animals,Anura,Bombesin,Bombesin: analogs \& derivatives,Bombesin: chemistry,Colloids,Colloids: chemistry,Electrodes,Nanoparticles,Nanoparticles: chemistry,Neurokinin B,Neurokinin B: analogs \& derivatives,Neurokinin B: chemistry,Oligopeptides,Oligopeptides: chemistry,Peptide Fragments,Peptide Fragments: chemistry,Pyrrolidonecarboxylic Acid,Pyrrolidonecarboxylic Acid: analogs \& derivatives,Pyrrolidonecarboxylic Acid: chemistry,Silver,Silver: chemistry,Spectrum Analysis, Raman,Spectrum Analysis, Raman: methods,Surface Properties},
month = apr,
number = {14},
pages = {4978--85},
pmid = {19296643},
title = {{The orientation of BN-related peptides adsorbed on SERS-active silver nanoparticles: comparison with a silver electrode surface.}},
